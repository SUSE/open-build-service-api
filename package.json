{
  "name": "@SUSE/open-build-service-api",
  "version": "0.0.1",
  "description": "Typescript wrapper for the API of the Open Build Service",
  "license": "MIT",
  "repository": "https://github.com/SUSE/open-build-service-api",
  "author": {
    "name": "Dan Čermák",
    "email": "dcermak@suse.com",
    "url": "https://github.com/dcermak/"
  },
  "keywords": [
    "open-build-service",
    "api",
    "api-wrapper",
    "api-client",
    "typescript"
  ],
  "files": ["lib"],
  "main": "lib/main.js",
  "types": "lib/main.d.ts",
  "scripts": {
    "format": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn run build",
    "build": "tsc --pretty",
    "clean": "rm -rf ./lib ./coverage/ ./.nyc_output ./documentation",
    "pretest": "yarn run build",
    "test": "mocha",
    "coverage": "nyc mocha",
    "watch": "yarn run build -- --watch",
    "watch:test": "yarn run test -- --watch",
    "documentation": "typedoc --theme minimal --out ./documentation src/"
  },
  "dependencies": {
    "config-ini-parser": "^1.3.5",
    "seek-bzip": "^2",
    "xml2js": "^0.4.22"
  },
  "devDependencies": {
    "@types/chai": "^4",
    "@types/chai-as-promised": "^7.1",
    "@types/chai-things": "^0.0",
    "@types/mocha": "^9",
    "@types/mock-fs": "^4",
    "@types/node": "^16",
    "@types/sinon": "^10",
    "@types/sinon-chai": "^3",
    "@types/xml2js": "^0.4",
    "@typescript-eslint/eslint-plugin": "^4",
    "@typescript-eslint/parser": "^4",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-things": "^0.2.0",
    "eslint": "^7",
    "mocha": "^9",
    "mock-fs": "^5",
    "nock": "^13",
    "nyc": "^15.1.0",
    "prettier": "^2",
    "sinon": "^11",
    "sinon-chai": "^3.5",
    "source-map-support": "^0.5",
    "ts-node": "^10",
    "typedoc": ">= 0.18",
    "typescript": ">= 3.7"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "prettier": {
    "trailingComma": "none"
  },
  "nyc": {
    "include": ["src/**/*.ts"],
    "exclude": ["lib"],
    "extension": [".ts"],
    "require": ["ts-node/register"],
    "reporter": ["html", "text", "lcov"],
    "all": true,
    "cache": false
  },
  "mocha": {
    "require": ["ts-node/register", "test/setup.js"],
    "spec": "test/**/*.test.ts",
    "extension": ["ts"]
  }
}
